<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  name="FlipkartCrossBrowserSuite" parallel="false"  thread-count="2">

  <test name="FlipkartTest - Chrome">
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="testScenario.FlipKartTest"/>
    </classes>
  </test>

  <test name="FlipkartTest - Edge">
    <parameter name="browser" value="edge"/>
    <classes>
      <class name="testScenario.FlipKartTest"/>
    </classes>
  </test>

</suite>


